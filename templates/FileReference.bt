
struct FileReference
{
    uint16 lowValue;
    uint16 highValue;
    uint16 zero;
};

string readFileReference(const FileReference& iFileReference)
{
    string result;   
    SPrintf(result, "%d", 0xFF00 * (iFileReference.highValue - 0x100) + (iFileReference.lowValue - 0x100) + 1);
    return result;
}